                                  rdfs:domain pproc:ContractObject ;
                                  rdfs:range pproc:ContractExecutionConditions ;
                                  rdfs:label "Condiciones de ejecución del contrato"@es ,
                                             "Contract execution conditions"@en .


###  http://contsem.unizar.es/def/sector-publico/pproc#contractModification
pproc:contractModification rdf:type owl:ObjectProperty ;
                           rdfs:domain pproc:Contract ;
                           rdfs:range pproc:ContractModification ;
                           rdfs:comment "Indica la modificación efectuada en un contrato."@es ,
                                        "Points to contract modifications being made."@en ;
                           rdfs:label "Contract modification"@en ,
                                      "Modificación de contrato"@es .


###  http://contsem.unizar.es/def/sector-publico/pproc#contractModificationConditions
pproc:contractModificationConditions rdf:type owl:ObjectProperty ,
                                              owl:FunctionalProperty ;
                                     rdfs:domain pproc:ContractObject ;
                                     rdfs:range pproc:ContractModificationConditions ;
                                     rdfs:label "Condiciones para la modificación del contrato"@es ,
                                                "Contract modification conditions"@en .


###  http://contsem.unizar.es/def/sector-publico/pproc#contractObject
pproc:contractObject rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain pproc:Contract ;
                     rdfs:range pproc:ContractObject ;
                     rdfs:label "Object of the contract"@en ,
                                "Objeto del contrato"@es .


###  http://contsem.unizar.es/def/sector-publico/pproc#contractOrProcedureExtinction
pproc:contractOrProcedureExtinction rdf:type owl:ObjectProperty ;
                                    rdfs:domain pproc:Contract ;
                                    rdfs:range pproc:ContractOrProcedureExtinction ;
                                    rdfs:comment "Abstract property pointing to extinction of contract or procedure. Use any of its subclasses."@en ,
                                                 """Propiedad abstracta que indica la extinción de un contrato por resolución, renuncia o abandono. O bien la extinción del procedimiento al ser declarado desierto.

Utilizar una de sus subclases."""@es ;
                                    rdfs:label "Contract or procedure extinction"@en ,
                                               "Extinción de contrato o de procedimiento"@es .


###  http://contsem.unizar.es/def/sector-publico/pproc#contractProcedureSpecifications
pproc:contractProcedureSpecifications rdf:type owl:ObjectProperty ,
                                               owl:FunctionalProperty ;
                                      rdfs:domain pproc:Contract ;
                                      rdfs:range pproc:ContractProcedureSpecifications ;
                                      rdfs:label "Contract procedure specifications"@en ,
                                                 "Especificaciones del procedimiento del contrato"@es .


###  http://contsem.unizar.es/def/sector-publico/pproc#contractResolution
pproc:contractResolution rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf pproc:contractOrProcedureExtinction ;
                         rdfs:range pproc:ContractResolution ;
                         rdfs:label "Contract resolution"@en ,
                                    "Resolución de contrato"@es .


###  http://contsem.unizar.es/def/sector-publico/pproc#contractTemporalConditions
pproc:contractTemporalConditions rdf:type owl:ObjectProperty ,
                                          owl:FunctionalProperty ;
                                 rdfs:domain pproc:ContractObject ;
                                 rdfs:range pproc:ContractTemporalConditions ;
                                 rdfs:label "Condiciones temporales del contrato"@es ,
                                            "Contract temporal conditions"@en .


###  http://contsem.unizar.es/def/sector-publico/pproc#contractingBody
pproc:contractingBody rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain pproc:Contract ;
                      rdfs:range org:Organization ;
                      rdfs:label "Contracting body"@en ,
                                 "Órgano de contratación"@es .


###  http://contsem.unizar.es/def/sector-publico/pproc#contractingBodyAgreement
pproc:contractingBodyAgreement rdf:type owl:ObjectProperty ;
                               rdfs:domain [ rdf:type owl:Class ;
                                             owl:unionOf ( pproc:ContractActivities
                                                           pproc:ContractModification
                                                           pproc:ContractOrProcedureExtinction
                                                           pproc:ContractProcedureSpecifications
                                                         )
                                           ] ;
                               rdfs:range pproc:ContractingBodyAgreement ;
                               rdfs:label "Acuerdo del órgano de contratación"@es ,
                                          "Contracting body agreement"@en .


###  http://contsem.unizar.es/def/sector-publico/pproc#criterion
pproc:criterion rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain pproc:CriterionScore ;
                rdfs:range pc:AwardCriterion ;
                rdfs:label "Criterio"@es ,
                           "Criterion"@en .


###  http://contsem.unizar.es/def/sector-publico/pproc#criterionScore
pproc:criterionScore rdf:type owl:ObjectProperty ;
                     rdfs:domain pc:Tender ;
                     rdfs:range pproc:CriterionScore ;
                     rdfs:comment "Especifica la valoración de un único criterio, por lo que se utilizará una vez por cada criterio de adjudicación presente en el contrato."@es ,
                                  "Property that points to an unique criterion and its score."@en ;
                     rdfs:label "Criterion score"@en ,
                                "Valoración de criterio"@es .


###  http://contsem.unizar.es/def/sector-publico/pproc#delegatingAuthority
pproc:delegatingAuthority rdf:type owl:ObjectProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain pproc:Contract ;
                          rdfs:range org:Organization ;
                          rdfs:comment "Indica la entidad que delega la licitación de este contrato."@es ,
                                       "Points to the authority that is delegating the contract."@en ;
                          rdfs:label "Delegating authority"@en ,
                                     "Entidad delegante"@es .


###  http://contsem.unizar.es/def/sector-publico/pproc#documentReference
pproc:documentReference rdf:type owl:ObjectProperty ;
                        rdfs:range foaf:Document ;
                        rdfs:comment """Enlace a documentación.

Propiedad similar a pc:attachment, pero sin dominio fijo, pues esta misma propiedad puede utilizarse con múltiples clases (e.g. pc:Contract ó contsem:TenderMeeting)"""@es ,
                                     "Links to external documents."@en ;
                        rdfs:label "Document reference"@en ,
                                   "Documentación"@es ;
                        rdfs:seeAlso pc:attachment .


###  http://contsem.unizar.es/def/sector-publico/pproc#electronicAuction
pproc:electronicAuction rdf:type owl:ObjectProperty ,
                                 owl:FunctionalProperty ;
                        rdfs:domain pproc:ContractProcedureSpecifications ;
                        rdfs:range pproc:ElectronicAuction ;
                        rdfs:label "Electronic auction"@en ,
                                   "Subasta electrónica"@es .


###  http://contsem.unizar.es/def/sector-publico/pproc#electronicAuctionPhase
pproc:electronicAuctionPhase rdf:type owl:ObjectProperty ;
                             rdfs:domain pproc:ElectronicAuction ;
                             rdfs:range pproc:ElectronicAuctionPhase ;
                             rdfs:label "Electronic auction phase"@en ,
                                        "Fase de subasta electrónica"@es .


###  http://contsem.unizar.es/def/sector-publico/pproc#electronicAuctionWeb
pproc:electronicAuctionWeb rdf:type owl:ObjectProperty ;
                           rdfs:domain pproc:ElectronicAuction ;
                           rdfs:range foaf:Document ;
                           rdfs:comment "Apunta a la web donde puede se realiza la subasta electrónica, o en su defecto, donde se pueda encontrar más información acerca de la subasta electrónica."@es ,
                                        "Property that indicated where the electronic auction is made or where we can look for more information about the auction."@en ;
                           rdfs:label "Electronic auction web"@en ,
                                      "Web de la subasta electrónica"@es .


###  http://contsem.unizar.es/def/sector-publico/pproc#estimatedValue
pproc:estimatedValue rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf pproc:price ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain pproc:ContractEconomicConditions ;
                     rdfs:range pproc:BundlePriceSpecification ;
                     rdfs:label "Estimated value of the contract"@en ,
                                "Valor estimado del contrato"@es ;
                     owl:backwardCompatibleWith pc:estimatedPrice .


###  http://contsem.unizar.es/def/sector-publico/pproc#feePrice
pproc:feePrice rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf pproc:price ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain pproc:ContractEconomicConditions ;
               rdfs:range pproc:BundlePriceSpecification ;
               rdfs:label "Canon"@es ,
                          "Fee"@en .


###  http://contsem.unizar.es/def/sector-publico/pproc#frameworkAgreement
pproc:frameworkAgreement rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain pproc:Contract ;
                         rdfs:range pproc:FrameworkAgreement ;
                         rdfs:comment """Property that indicates the framework agreement in which the contract consists.

This property is used when the actual contract consist in the declaration of a framework agreement or a dynamic purchasing system."""@en ,
                                      """Señala al acuerdo marco o sistema dinámico de contratación en el cual consiste el contrato.

Esta propiedad se utiliza cuando el actual contrato consista en la declaración de un acuerdo marco o un sistema dinámico de contratación."""@es ;
                         rdfs:label "Acuerdo marco o sistema dinámico de contratación"@es ,
                                    "Framework agreement"@en .


###  http://contsem.unizar.es/def/sector-publico/pproc#frameworkAgreementDerivativeContract
pproc:frameworkAgreementDerivativeContract rdf:type owl:ObjectProperty ,
                                                    owl:InverseFunctionalProperty ;
                                           rdfs:domain pproc:ConclusionContract ;
                                           rdfs:range pproc:DerivativeContract ;
                                           rdfs:label "Contrato derivado de un acuerdo marco"@es ,
                                                      "Framework agreement derivative contract"@en .


###  http://contsem.unizar.es/def/sector-publico/pproc#fullMember
pproc:fullMember rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf foaf:member ;
                 rdfs:domain pproc:Committee ;
                 rdfs:range pproc:CommitteeMember ;
                 rdfs:comment "Miembro titular en una mesa de contratación."@es ,
                              "Points to a full member of a committee."@en ;
                 rdfs:label "Full member"@en ,
                            "Miembro titular"@es .


###  http://contsem.unizar.es/def/sector-publico/pproc#informationKind
pproc:informationKind rdf:type owl:ObjectProperty ;
                      rdfs:domain pproc:InformationProvider ;
                      rdfs:range skos:Concept ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty skos:inScheme ;
                                   owl:hasValue pproc:InformationKindScheme
                                 ] ;
                      rdfs:comment """Indica el tipo de información que suministra un proveedor de información.

Es posible utilizar esta propiedad numerosas veces para indicar diversos tipos de información."""@es ,
                                   "Property pointing to the type of the information is provided."@en ;
                      rdfs:label "Information kind"@en ,
                                 "Tipo de información"@es ;
                      rdfs:seeAlso pproc:InformationKindScheme .


###  http://contsem.unizar.es/def/sector-publico/pproc#item
pproc:item rdf:type owl:ObjectProperty ;
