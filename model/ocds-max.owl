#################################################################
#    Object Properties
#################################################################

###  http://data.businessgraph.io/ontology#adminUnit
ebg:adminUnit rdf:type owl:ObjectProperty ;
              schema:domainIncludes locn:Address ;
              schema:rangeIncludes ebg:LAURegion ,
                                   ramon:NUTSRegion ;
              rdfs:isDefinedBy ebg: ;
              rdfs:label "admin unit" ;
              skos:definition "All administrative units related to an address." ;
              skos:scopeNote "Union of locn:adminUnitL1..2 and ebg:adminUnitL3..6. Used for faceting by place." .


###  http://data.businessgraph.io/ontology#adminUnitL3
ebg:adminUnitL3 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf ebg:adminUnit ;
                schema:domainIncludes locn:Address ;
                schema:rangeIncludes ramon:NUTSRegion ;
                rdfs:isDefinedBy ebg: ;
                rdfs:label "admin unit level 3" ;
                skos:definition "NUTS2 (administrative region) of the address." ;
                skos:example "nuts:ITC2 Valle d'Aosta/Vallée d'Aoste" ,
                             "nuts:ITH2 Provincia Autonoma di Trento" ;
                skos:scopeNote "Use a valid NUTS region. Eg Trentino-Alto Adige/Sudtirol (taken from the Geonames hierarchy above) is not a NUTS region." .


###  http://data.businessgraph.io/ontology#adminUnitL4
ebg:adminUnitL4 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf ebg:adminUnit ;
                schema:domainIncludes locn:Address ;
                schema:rangeIncludes ramon:NUTSRegion ;
                rdfs:isDefinedBy ebg: ;
                rdfs:label "admin unit level 4" ;
                skos:definition "NUTS3 (Province) of the address." ;
                skos:example "nuts:ITC20 Aosta" ,
                             "nuts:ITH31 Verona" .


###  http://data.businessgraph.io/ontology#adminUnitL5
ebg:adminUnitL5 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf ebg:adminUnit ;
                schema:domainIncludes locn:Address ;
                schema:rangeIncludes ebg:LAURegion ;
                rdfs:isDefinedBy ebg: ;
                rdfs:label "admin unit level 5" ;
                skos:definition "LAU1 (Municipality/Commune/Settlement) of the address." ;
                skos:scopeNote "Some countries (e.g. Bulgaria) use both LAU1 and LAU2 levels. Others (e.g. Italy) use only LAU2." .


###  http://data.businessgraph.io/ontology#adminUnitL6
ebg:adminUnitL6 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf ebg:adminUnit ;
                schema:domainIncludes locn:Address ;
                schema:rangeIncludes ebg:LAURegion ;
                rdfs:isDefinedBy ebg: ;
                rdfs:label "admin unit level 6" ;
                skos:definition "LAU2 (Municipality/Commune/Settlement) of the address." ;
                skos:scopeNote "Some countries (e.g. Bulgaria) use both LAU1 and LAU2 levels. Others (e.g. Italy) use only LAU2." .


###  http://data.businessgraph.io/ontology#geoResolution
ebg:geoResolution rdf:type owl:ObjectProperty ;
                  schema:domainIncludes schema:GeoCoordinates ;
                  rdfs:isDefinedBy ebg: ;
                  rdfs:label "geocoordinate resolution" ;
                  skos:definition "Resolution of a geographic coordinate location: from <resolution/L0> continent (least precise) to <resolution/L10> individual house number (most precise)." ;
                  skos:scopeNote "If geo coordinates are present, resolution must be present too, even if it's a guess. There is no default value." ,
                                 "Values must be mapped to the <resolution/> ConceptScheme (lookup list)." .


###  http://data.businessgraph.io/ontology#identifierWebResource
ebg:identifierWebResource rdf:type owl:ObjectProperty ;
                          schema:domainIncludes ebg:IdentifierSystem ;
                          schema:rangeIncludes ebg:IdentifierWebResource ;
                          rdfs:isDefinedBy ebg: ;
                          rdfs:label "identifier web resource" ;
                          skos:definition "Web resource associated with an identifier system" ;
                          skos:example "'https://opencorporates.com/companies/{}'      with MIME type 'text/html'" ,
                                       "'https://opencorporates.com/companies/{}.json' with MIME type 'application/json'" ,
                                       "'https://opencorporates.com/companies/{}.rdf'  with MIME type 'application/rdf+xml'" ,
                                       "'https://opencorporates.com/companies/{}.xml'  with MIME type 'application/xml'" ;
                          skos:scopeNote "Specifies a template that can be used uniformly to build URLs for all identifiers in the system." .


###  http://data.businessgraph.io/ontology#validationRule
ebg:validationRule rdf:type owl:ObjectProperty ;
                   schema:domainIncludes ebg:IdentifierSystem ;
                   schema:rangeIncludes rdf:Resource ;
                   rdfs:isDefinedBy ebg: ;
                   rdfs:label "validation rule" ;
                   skos:definition "URL providing human or machine-readable rule(s) for validating identifiers in the system" ;
                   skos:example "http://bsv-bg.com/контролни-цифри-ползвани-в-българия/ describes the BG EIK checksum algorithm in HTML" ,
                                "http://www.nsi.bg/sites/default/files/konkursi/RGP_OPAK_2014_Annex_6.pdf describes the BG EIK checksum algorithm in PDF. It is by an official source, but is less convenient" ;
                   skos:scopeNote "Can be in the form of webpage, PDF document, RDF shape, etc. Multiple values about the same validation rule are ok." .


###  http://data.businessgraph.io/ontology#webResource
ebg:webResource rdf:type owl:ObjectProperty ;
                schema:domainIncludes ebg:IdentifierSystem ,
                                      void:Dataset ;
                schema:rangeIncludes ebg:WebResource ;
                rdfs:isDefinedBy ebg: ;
                rdfs:label "web resource" ;
                skos:definition "Web resource associated with an identifier system." ;
                skos:example "http://opendata.government.bg/dataset/tbprobckn-pernctbp is the open data download site for BG TR." ,
                             "https://brra.bg is the homepage of BG TR." ,
                             "https://kbopub.economie.fgov.be/kbopub/zoeknummerform.html is the search URL for BE Banque-Carrefour des Entreprises." .


