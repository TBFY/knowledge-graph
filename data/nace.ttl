        skos:scopeNote     "This class includes:\n- manufacture of powdered mortars"@en ;
        ebg:exclusionNote  "This class excludes:\n- manufacture of refractory mortars, see 23.20\n- manufacture of dry-mixed concrete and mortars, see 23.63"@en .

nace:23.65  rdf:type    skos:Concept ;
        skos:inScheme   nace: ;
