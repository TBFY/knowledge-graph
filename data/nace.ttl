        ebg:order          398747 ;
        ebg:level          4 ;
        skos:prefLabel     "Cutting, shaping and finishing of stone"@en ;
        skos:scopeNote     "This class includes:\n- cutting, shaping and finishing of stone for use in construction, in cemeteries, on roads, as roofing etc.\n- manufacture of stone furniture"@en ;
        ebg:exclusionNote  "This class excludes:\n- activities carried out by operators of quarries, e.g. production of rough cut stone, see 08.11\n- production of millstones, abrasive stones and similar products, see 23.9"@en .

nace:23.9  rdf:type     skos:Concept ;
